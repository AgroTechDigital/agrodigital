{
  "name": "piquete",
  "plural": "piquetes",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    }
  },
  "properties": {
    "codigo": {
      "type": "string"
    },
    "nome": {
      "type": "string",
      "required": true
    },
    "coordenadas": {
      "type": [
        "geopoint"
      ]
    },
    "area": {
      "type": "number"
    },
    "tipo": {
      "type": "string",
      "required": true,
      "default": "extensivo"
    },
    "tipoCapim": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "recuperacao"
    },
    "tempoRecuperacao": {
      "type": "number"
    },
    "capacidadeUa": {
      "type": "number"
    },
    "historicoUa": {
      "type": [
        "object"
      ]
    }
  },
  "validations": [],
  "relations": {
    "modulo": {
      "type": "belongsTo",
      "model": "modulo",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "eventos": {
      "type": "hasMany",
      "model": "piqueteEventos",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
