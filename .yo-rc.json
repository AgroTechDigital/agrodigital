{
  "generator-fireloop": {
    "version": "1.0.0-beta.2.7",
    "clients": {
      "fireloop": {
        "path": "./fireloop",
        "type": "server"
      },
      "ionicapp": {
        "path": "./ionicapp",
        "type": "ng2ionic",
        "name": "ionicapp",
        "sdkPath": "ionicapp\\src\\app\\shared\\sdk"
      }
    },
    "promptValues": {
      "sdkFeatures": [
        "Enable PubSub + IO + FireLoop functionality",
        "Generate ONLY FireLoop SDK + Auth Services"
      ]
    }
  }
}